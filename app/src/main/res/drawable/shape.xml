<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- fill/color -->
    <solid android:color="@android:color/transparent" />
    <!-- Control the width and height of the shape -->
    <!-- Control the radius of each corners -->
    <stroke
        android:width="0.5dp"
        android:color="#070707"/>
    <corners
        android:bottomLeftRadius="3.5dp"
        android:bottomRightRadius="3.5dp"
        android:topLeftRadius="3.5dp"
        android:topRightRadius="3.5dp" />
</shape>